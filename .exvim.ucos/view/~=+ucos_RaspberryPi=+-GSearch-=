let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
argglobal
nnoremap <buffer> <silent>  :call exgsearch#confirm_select('')
nnoremap <buffer> <silent>   :call exgsearch#toggle_zoom()
nnoremap <buffer> <silent> \fd :exec 'ReverseFilterFile ' . @/
nnoremap <buffer> <silent> \d :exec 'ReverseFilter ' . @/
nnoremap <buffer> <silent> \fr :exec 'FilterFile ' . @/
nnoremap <buffer> <silent> \r :exec 'Filter ' . @/
nnoremap <buffer> <silent> \ :EXGSearchClose
let s:cpo_save=&cpo
set cpo&vim
nnoremap <buffer> <silent> <S-2-LeftMouse> :call exgsearch#confirm_select('shift')
nnoremap <buffer> <silent> <S-CR> :call exgsearch#confirm_select('shift')
nnoremap <buffer> <silent> <2-LeftMouse> :call exgsearch#confirm_select('')
nnoremap <buffer> <silent> <F1> :call exgsearch#toggle_help()
let &cpo=s:cpo_save
unlet s:cpo_save
setlocal keymap=
